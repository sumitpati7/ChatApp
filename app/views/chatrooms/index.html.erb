<div class="min-h-screen bg-gray-100 dark:bg-gray-900 text-gray-800 dark:text-gray-200 p-6">
  <!-- Header -->
  <header class="flex items-center justify-between mb-8">
    <h1 class="text-2xl font-bold text-indigo-600 dark:text-indigo-400">ChatRoom Dashboard</h1>
    <div>
      <%= link_to current_user.email, appendUserAttributes_users_path, 
        class: "text-sm text-indigo-600 hover:text-indigo-800 dark:text-indigo-400 dark:hover:text-indigo-500 underline" %>
    </div>
  </header>

  <!-- Users Section -->
  <section class="mb-8">
    <h2 class="text-xl font-semibold border-b border-gray-300 dark:border-gray-700 pb-2 mb-4">Users</h2>
    <div class="bg-white dark:bg-gray-800 rounded-lg shadow p-4">
      <%= render @users %>
    </div>
  </section>

  <!-- ChatRooms Section -->
  <section>
    <h2 class="text-xl font-semibold border-b border-gray-300 dark:border-gray-700 pb-2 mb-4">ChatRooms</h2>
    <div class="bg-white dark:bg-gray-800 rounded-lg shadow p-4">
      <%= render @chatrooms %>
    </div>
  </section>
</div>
